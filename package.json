{
    "name": "@zhinjs/cli",
    "version": "0.2.8",
    "description": "zhin 脚手架",
    "bin": {
        "zhin": "lib/index.js"
    },
    "scripts": {
        "start": "zhin start",
        "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
        "pub": "npm publish --access public",
        "test": "node ./lib/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zhinjs/cli.git"
    },
    "keywords": [
        "zhin",
        "cli",
        "service"
    ],
    "author": "凉菜",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zhinjs/cli/issues"
    },
    "homepage": "https://github.com/zhinjs/cli#readme",
    "devDependencies": {
        "@types/inquirer": "^9.0.2",
        "@types/js-yaml": "^4.0.5",
        "@types/koa": "^2.13.5",
        "@types/node": "latest",
        "ts-node-dev": "latest",
        "tsc-alias": "^1.8.2",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.4"
    },
    "files": [
        "/lib/**/*.js",
        "/lib/**/*.d.ts",
        "/lib/**/LICENSE"
    ],
    "dependencies": {
        "axios": "^1.1.2",
        "cac": "^6.7.14",
        "inquirer": "~8.2.4",
        "js-yaml": "^4.1.0"
    }
}
